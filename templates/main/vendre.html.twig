{% extends 'base.html.twig' %}

{% block title %}Vendre un bien{% endblock %}

{% block meta %}
    <meta name="description" content="Estimation, mise en valeur de votre bien, accompagnement, visites : optimiser la vente de votre bien est notre priorité.">
{% endblock %}

{% block body %}
    <section class="container-main">
        <div class="container-header-main">
                {{ include('_include/_mainNavigationBlack.html.twig') }}
    </div>
</section>
<section class="bloc-contact container-small">
