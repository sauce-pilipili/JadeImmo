{% extends 'base.html.twig' %}

{% block title %}Jade Immobilier-Vendre un bien{% endblock %}

{% block body %}
    <section class="container-main">
        <div class="container-header-main">
                {{ include('_include/_mainNavigationBlack.html.twig') }}
    </div>
</section>
